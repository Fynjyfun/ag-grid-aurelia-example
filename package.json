    "connect-history-api-fallback": "1.2.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-changed-in-place": "2.0.3",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "1.2.2",
    "gulp-sourcemaps": "2.0.0-alpha",
    "gulp-notify": "2.2.0",
    "minimatch": "3.0.2",
    "through2": "2.0.1",
