{
  "name": "ag-grid-aurelia-example",
  "description": "An Aurelia client application.",
  "version": "17.1.0",
  "scripts": {
    "build": "au build",
    "copy-to-docs": "copyfiles index.html docs/ && ncp scripts/ docs/scripts/ && ncp src/components docs/components && ncp images/ docs/images/ && mkdir -p docs/node_modules/bootstrap/dist && mkdir -p docs/node_modules/jquery/dist && ncp node_modules/bootstrap/dist docs/node_modules/bootstrap/dist && ncp node_modules/bootstrap/dist docs/node_modules/jquery/dist",
    "copy-to-ag-docs": "npm run build && npm run copy-to-docs && ncp docs ../ag-grid-docs/src/aurelia-example/",
    "start": "au run",
    "watch" : "au run --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid-aurelia-example.git"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "ag-grid": "^18.0.0",
    "ag-grid-enterprise": "^18.0.0",
    "ag-grid-aurelia": "^18.0.0",
    "aurelia-bootstrapper": "2.1.1",
    "aurelia-fetch-client": "1.1.3",
    "aurelia-animator-css": "1.0.4",
    "aurelia-templating": "1.6.0",
    "aurelia-templating-resources": "1.5.4",
    "aurelia-framework": "1.1.5",
    "aurelia-router": "1.4.0",
    "aurelia-history": "1.1.0",
    "bluebird": "3.5.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "aurelia-cli": "0.32.0",
    "aurelia-testing": "1.0.0-beta.4.0.0",
